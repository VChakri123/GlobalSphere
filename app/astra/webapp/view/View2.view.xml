<mvc:View
	height="100%"
	controllerName="com.app.astra.controller.View2"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox id="_IDGenVBox1" class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormChange354"
			editable="true"
			layout="ResponsiveGridLayout"
			title="Star With The Entire Details"
			labelSpanXL="3"
			labelSpanL="3"
			labelSpanM="3"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			emptySpanS="0"
			columnsXL="1"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
            
			<Bar id="_IDGenBar">
				<contentRight>
					<!-- <Button id="edit" text="Edit" type="Emphasized" enabled="true" press="handleEditPress"/>
					<Button id="save" text="Save" type="Success" visible="true" press="handleSavePress"/> -->
					<Button id="back" text="Back" type="Up" visible="true" press="handleCancelPress"/>
				</contentRight>
			</Bar>
		
			<f:content>

			    <Label id="_IDGenLabel0" text="ID" />
				<Input id="IDD" editable="false" />

				<Label id="_IDGenLabel" text="StarName" />
				<Input id="StarName"  editable="false" />

				<Label id="_IDGenLabel1" text="Constellation" />
				<Input id="Constellation"  editable="false" />
				
				<Label id="_IDGenLabel2" text="	Description"/>
				<Input id="Description" editable="false"/>	
				
				<Label id="_IDGenLabel3" text="Type" />
				<Input id="Type" editable="false"  />	

				<!-- <Select id="Type" selectedKey="{Type}"  visible="false"/> -->
					

			</f:content>
		</f:SimpleForm>
	</VBox>
</mvc:View>